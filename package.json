{
  "name": "rosi-vscode",
  "displayName": "Rosi Language Support",
  "description": "Rosi language support for vscode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.103.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "rosi",
        "aliases": [
          "Rosi",
          "rosi"
        ],
        "extensions": [
          ".ro"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "rosi",
        "scopeName": "source.rosi",
        "path": "./syntaxes/rosi.tmLanguage.json"
      }
    ]
  }
}
